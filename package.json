{
  "name": "@paulohenriquevn/sdkagent",
  "version": "2.0.1",
  "description": "CLI Agent Tools SDK - 30+ production-ready tools with direct access, no framework dependencies",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "BREAKING-CHANGES.md",
    "MIGRATION-GUIDE.md"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "npx jest --config src/tools/tests/jest.config.js",
    "lint": "npx eslint src/**/*.ts --fix",
    "typecheck": "npx tsc --noEmit"
  },
  "keywords": [
    "cli-agent",
    "ai-tools",
    "cli-tools",
    "automation",
    "sdk",
    "typescript",
    "direct-access",
    "no-dependencies"
  ],
  "author": "CLI Agent Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/paulohenriquevn/sdkagent.git"
  },
  "bugs": {
    "url": "https://github.com/paulohenriquevn/sdkagent/issues"
  },
  "homepage": "https://github.com/paulohenriquevn/sdkagent#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.0",
    "fs-extra": "^11.2.0",
    "openai": "^4.104.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.0.0"
  }
}
