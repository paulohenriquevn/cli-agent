{
  "name": "llm-code-agent",
  "version": "1.0.0",
  "description": "Autonomous LLM Code Agent with OpenRouter - Updated Dependencies",
  "main": "test-cli-tools.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/login-server.js",
    "dev": "ts-node login-server.ts",
    "test": "cd src/tools/tests && npx jest --config jest.config.js",
    "lint": "npx eslint src/**/*.ts --fix",
    "lint:check": "npx eslint src/**/*.ts",
    "typecheck": "npx tsc --noEmit",
    "typecheck:watch": "npx tsc --noEmit --watch",
    "check": "npm run lint:check && npm run typecheck",
    "fix": "npm run lint && npm run typecheck",
    "validate": "npm run check && npm run test:summary"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "hnswlib-node": "^1.4.2",
    "inquirer": "^9.2.0",
    "jsonwebtoken": "^9.0.0",
    "openai": "^4.104.0",
    "ora": "^5.4.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
